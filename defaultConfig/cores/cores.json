{
    "version": "1.0.0",
    "attributes": {
        "CoreName": {
            "label": "Core Name",
            "type": "string",
            "validation": "^[A-Za-z0-9]+$",
            "tooltip": "Current core name"
        },
        "bootOs": {
            "label": "Boot OS",
            "type": "bool",
            "tooltip": "Will current core boot the operating system?"
        },
        "isCosmos": {
            "label": "Is CosmOS core",
            "type": "bool",
            "tooltip": "Runs current core the CosmOS operating system?"
        },
        "unmappedDataMemory": {
            "inherit": "memories/:memory",
            "label": "Memory location of the unmapped data"
        },
        "unmappedDataSize": {
            "label": "Unmapped data section size",
            "type": "int",
            "min": 0,
            "max": 2048,
            "tooltip": "Size of the unmapped data section"
        },
        "unmappedDataLowAddress": {
            "placeholder": true,
            "type": "int"
        },
        "unmappedDataHighAddress": {
            "placeholder": true,
            "type": "int"
        },
        "cpu": {
            "type": "parentReference"
        }
    },
    "elements": {}
}