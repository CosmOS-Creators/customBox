{{"\n"}}{% for item in model.programs.iterator -%}
{% for task in item.programTasks -%}
void wrapped_{{task.name}}(void)
{
    {% if task.isSysJob -%}
    sysCalls_os(SYSCALL_SYSJOBS_DISPATCHER);
    sysCalls_os(SYSCALL_SCHEDULABLE_SETEXECUTIONSTATETOFINISHED);
    {% elif task.isIdle -%}
    {{task.name}}();
    {% else -%}
    {{task.name}}();
    sysCalls_os(SYSCALL_SCHEDULABLE_SETEXECUTIONSTATETOFINISHED);
    {% endif -%}
    for(;;);
};
{% endfor -%}
{% endfor -%}

{% for item in model.programs.iterator -%}
{% for thread in item.programThreads -%}
void wrapped_{{thread.name}}(void)
{
    while ( 1 )
    {
        {{thread.name}}();
    }
};
{% endfor -%}
{% endfor -%}
